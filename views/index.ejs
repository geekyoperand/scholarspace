<script async>
            $(document).ready(function() {
                $('.counter-up-number').each(function() {
                    var $this = $(this);
                    var targetValue = $this.attr('data-value');
                    var isDecimal = $this.hasClass('counter-decimal');

                    // Parse the target value as a float if it's decimal, or as an integer otherwise
                    targetValue = isDecimal ? parseFloat(targetValue) : parseInt(targetValue);

                    if (!isNaN(targetValue)) {
                        $({
                            counterValue: 0
                        }).animate({
                            counterValue: targetValue
                        }, {
                            duration: 2000,
                            // Change this to adjust the animation duration
                            easing: 'swing',
                            step: function() {
                                if (isDecimal) {
                                    // Display the counter value with one decimal place if it's decimal
                                    $this.text(this.counterValue.toFixed(1));
                                } else {
                                    // Display the counter value as an integer otherwise
                                    $this.text(Math.round(this.counterValue));
                                }
                            },
                            complete: function() {
                                if (isDecimal) {
                                    // Display the target value with one decimal place if it's decimal
                                    $this.text(targetValue.toFixed(1));
                                } else {
                                    // Display the target value as an integer otherwise
                                    $this.text(targetValue);
                                }
                            }
                        });
                    } else {
                        $this.text('Invalid input');
                    }
                });
            });
        </script>
